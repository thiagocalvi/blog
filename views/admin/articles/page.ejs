<%- include ('../../partials/header.ejs') %>
<body>
    <%- include ('../../partials/homenavbar.ejs') %>
    <div class="container">
        <br>
        <% result.articles.rows.forEach(article => {%>
            <div class="card">
                <div class="card-header">
                    <h2><%= article.title%></h2>
                </div>
                <div class="card-body">
                    <a class="btn btn-success" href="/<%= article.slug %>">Ler artigo</a>
                    <br>
                    <p>Publicado em - <%= article.createdAt%></p>
                </div>
            </div>
            <br>
        <%})%>
    </div>
</body>
<%- include ('../../partials/footer.ejs') %>